<section class="post">
    <div class="post__inner">
        <div class="post__header">
            <%- partial('_partial/header') %>
        </div>
        <div class="post__body">
            <article class="post__mainland">
                <div class="post__header">
                    <h1 class="post__title"><%= page.title %></h1>
                    <div class="post__meta">
                        <%- partial('_partial/meta', {writeAt: page.date, categories: page.categories}) %>
                    </div>
                </div>
                <div class="post__contents">
                    <%- partial('_partial/contents', {contents: page.content}) %>
                </div>
                <div class="post__footer">
                    <div class="post__tags">
                        <%- partial('_partial/tags', {tags: page.tags}) %>
                    </div>
                    <div class="post__navigation">
                        <%- partial('_partial/post-navigation', {post: page}) %>
                    </div>
                </div>
            </article>
            <div class="post__attachment">
                <% if (page.comments && theme.disqus_shortname) { %>
                <div id="disqus_thread"></div>
                <noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                <% } %>
            </div>
        </div>
    </div>
</section>
