<div class="tags">
  <ul class="tags__list">
    <% if(tags && tags.length > 0) { %>
      <% tags.data.forEach(tag => { %>
        <li class="tags__item">
          <a class="tags__link" href="<%= url_for(tag.path) %>" >
            #<%= tag.name %>
          </a>
        </li>
      <% }); %>
    <% } %>
  </ul>
</div>
