{
  "name": "coderifleman-blog",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.4.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "scss-lint -c .scss-lint.yml && eslint ./themes/mooji/source/_js/**",
    "watch": "npm run lint && webpack --watch",
    "build:source": "npm run lint && webpack",
    "build:public": "hexo clean && hexo generate",
    "build": "npm run build:source && npm run build:public",
    "deploy": "rm -rf ./public/assets ./themes/mooji/source/assets/{css,fonts,js} && NODE_ENV=production && npm run build && hexo deploy",
    "server": "hexo server"
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "css-loader": "0.28.7",
    "eslint": "4.13.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "node-sass": "4.7.2",
    "postcss-loader": "2.0.9",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0"
  },
  "dependencies": {
    "font-awesome": "4.7.0",
    "hexo": "3.4.3",
    "hexo-deployer-git": "0.3.1",
