{
  "name": "coderifleman-blog",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.2.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "scss-lint -c .scss-lint.yml && eslint ./themes/mooji/source/_js/**",
    "watch": "npm run lint && webpack --watch",
    "build:source": "npm run lint && webpack",
    "build:public": "hexo clean && hexo generate",
    "build": "npm run build:source && npm run build:public",
    "deploy": "rm -rf ./public/assets ./themes/mooji/source/assets/{css,fonts,js} && NODE_ENV=production && npm run build && hexo deploy",
    "server": "hexo server"
  },
  "devDependencies": {
    "autoprefixer": "6.6.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-preset-es2015": "6.18.0",
    "css-loader": "0.26.1",
    "eslint": "3.12.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "node-sass": "4.1.1",
    "postcss-loader": "1.2.1",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0"
  },
  "dependencies": {
    "font-awesome": "4.7.0",
    "hexo": "3.2.2",
    "hexo-deployer-git": "0.2.0",
